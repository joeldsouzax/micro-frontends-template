{
  "name": "functional-typescript",
  "version": "1.0.0",
  "description": "functional typescript learning and examples",
  "main": "index.ts",
  "author": "Joel D'Souza <joeldsouzax@seamless.se>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build": "webpack --config webpack.config.ts --watch",
    "dev:run": "nodemon --watch build --exec \"node build/bundle.js\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js, .jsx, .ts, .tsx",
    "pretty:check": "prettier --check .",
    "pretty:write": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild-loader": "^2.18.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
